{
    "name": "open-solid/arch-viewer",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Export and visualize your OpenSolid project architecture",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpstan": "^2.1",
        "symfony/console": "^7.4|^8.0",
        "symfony/framework-bundle": "^7.4|^8.0"
    },
    "require-dev": {
        "open-solid/core": "@dev",
        "phpunit/phpunit": "^12.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OpenSolid\\ArchViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSolid\\ArchViewer\\Tests\\": "tests/"
        }
    }
}
