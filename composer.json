{
    "name": "open-solid/doc",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Doc generator for your OpenSolid projects",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.5",
        "open-solid/core": "dev-main",
        "opis/json-schema": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpstan": "^2.1",
        "symfony/console": "^7.4|^8.0",
        "symfony/framework-bundle": "^7.4|^8.0"
    },
    "require-dev": {
        "api-platform/core": "^4.2",
        "phpunit/phpunit": "^12.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OpenSolid\\Doc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSolid\\Doc\\Tests\\": "tests/",
            "App\\": "tests/Unit/Fixtures/src/"
        }
    }
}
